{
  "name": "@toolbuilder/rollup-plugin-create-pack-file",
  "version": "0.1.3",
  "description": "Create a pack file and move it somewhere else for testing.",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "pack"
  ],
  "homepage": "https://github.com/toolbuilder/rollup-plugin-create-pack-file#readme",
  "bugs": {
    "url": "https://github.com/toolbuilder/rollup-plugin-create-pack-file/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toolbuilder/rollup-plugin-create-pack-file.git"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "check": "run-s check:*",
    "check:packagejson": "npmPkgJsonLint .",
    "check:packfile": "rollup --config rollup.test.config.js",
    "check:src": "eslint src test",
    "check:test": "run-s test",
    "check:uncommitted": "uncommitted",
    "prerelease": "run-s check",
    "release": "standard-version",
    "test": "tape -r esm \"test/*test.js\""
  },
  "eslintConfig": {
    "extends": "@toolbuilder/eslint-config"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "pkg-dir": "^5.0.0"
  },
  "devDependencies": {
    "@toolbuilder/eslint-config": ">=0.1.4",
    "@toolbuilder/package-json-lint-config": "^0.1.0",
    "@toolbuilder/rollup-plugin-commands": "^0.1.2",
    "eslint": ">=7.31.0",
    "npm-package-json-lint": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.53.2",
    "rollup-plugin-create-test-package-json": "^0.1.3",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-relative-to-package": "^0.1.3",
    "standard-version": "^9.3.1",
    "tape": "^5.2.2",
    "uncommitted": "^1.0.0",
    "zora": "^4.1.0"
  },
  "npmpackagejsonlint": {
    "extends": "@toolbuilder/package-json-lint-config"
  }
}

{
  "name": "@toolbuilder/rollup-plugin-create-pack-file",
  "description": "create a pack file and move it somewhere else for testing",
  "version": "0.1.1",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "pack"
  ],
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "npm-run-all --serial test lint",
    "lint": "eslint src test",
    "test": "tape -r esm \"test/*.test.js\""
  },
  "eslintConfig": {
    "extends": "@toolbuilder/eslint-config"
  },
  "dependencies": {
    "esm": "^3.2.25",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.1",
    "pkg-dir": "^4.2.0"
  },
  "devDependencies": {
    "@toolbuilder/eslint-config": ">=0.1.2",
    "eslint": ">=6.8.0",
    "npm-run-all": "^4.1.5",
    "tape": "^5.0.0",
    "zora": "^3.1.8"
  }
}
